<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proyectoindividual">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_shop"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_shop_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProyectoIndividual">
        <activity android:name=".cliente.DetallesVentasCliente" android:parentActivityName=".cliente.HistorialCliente"></activity>
        <activity android:name=".cliente.HistorialCliente" android:parentActivityName=".cliente.MainCliente"/>
        <activity
            android:name=".admin.HistorialAdmin"
            android:parentActivityName=".admin.MainAdmin" />
        <activity
            android:name=".admin.DetalleVentas"
            android:parentActivityName=".admin.HistorialAdmin" />
        <activity
            android:name=".Historial"
            android:parentActivityName=".admin.MainAdmin" />
        <activity
            android:name=".cliente.VenderProducto"
            android:parentActivityName=".cliente.MainCliente" />
        <activity
            android:name=".cliente.InfoProducto"
            android:parentActivityName=".cliente.MainCliente" />
        <activity
            android:name=".admin.DetallesProducto"
            android:parentActivityName=".admin.MainAdmin" />
        <activity android:name=".admin.AgregarImagen" />
        <activity
            android:name=".admin.AgregarProducto"
            android:parentActivityName=".admin.MainAdmin" />
        <activity android:name=".cliente.MainCliente" />
        <activity android:name=".admin.MainAdmin" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>